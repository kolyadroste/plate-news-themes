{
    "name": "atomicplan/plate-news-themes",
    "version": "0.5.0",
    "type": "typo3-cms-extension",
    "description": "provides fluid templates for the news extension",
    "authors": [
        {
            "name": "Kolya von Droste zu Vischering",
            "role": "Developer"
        }
    ],
    "require": {
        "atomicplan/plate-atomic-assets": "^0.5",
        "georgringer/news": "^11"
    },
    "autoload": {
        "psr-4": {
            "Atomicplan\\PlateNewsThemes\\": "Classes/"
        }
    },
    "replace": {
        "typo3-ter/atomicplan/plate-news-themes": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "plate_news_themes"
        }
    }
}